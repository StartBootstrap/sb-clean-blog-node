{
    "name": "sb-clean-blog-node",
    "version": "1.0.0",
    "description": "node server for sb-clean-blog-angular",
    "main": "_transpiled/app.js",
    "scripts": {
        "build": "npm run webpack",
        "clean": "npm run ts-node scripts/clean",
        "cli": "npm run ts-node -- node_modules/typeorm/cli",
        "db:initialize": "npm run ts-node -- scripts/db/db-initialize.ts && npm run db:seed",
        "db:migration:generate": "npm run cli -- migration:generate",
        "db:migration:run": "npm run cli -- migration:run",
        "db:reset": "npm run ts-node -- scripts/db/db-reset.ts && npm run db:seed",
        "db:seed": "npm run ts-node scripts/db/db-seed.ts",
        "db:uuid": "npm run ts-node scripts/db/db-uuid.ts",
        "debug:brk": "nodemon -x 'npm run build && npm run node:inspect-brk'",
        "debug": "nodemon -x 'npm run build && npm run node:inspect'",
        "docker:bash": "npm run ts-node -- scripts/docker/docker-bash.ts",
        "docker:build": "npm run ts-node -- scripts/docker/docker-build.ts",
        "docker:run": "npm run ts-node -- scripts/docker/docker-run.ts",
        "link": "npm link @start-bootstrap/sb-clean-blog-shared-types",
        "lint:check": "tslint-config-prettier-check ./tslint.json",
        "lint:fix": "tslint -p . -c tslint.json --fix",
        "lint": "tslint -p . -c tslint.json",
        "node:inspect-brk": "export TS_NODE_PROJECT=src/tsconfig.app.json && export TS_NODE_FILES=true && node --inspect-brk -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/index.ts",
        "node:inspect": "export TS_NODE_PROJECT=src/tsconfig.app.json && export TS_NODE_FILES=true && node --inspect -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/index.ts",
        "serve:coverage": "static-server ./coverage/lcov-report 9754",
        "start": "npm run clean && nodemon -x 'npm run ts-node -- --files -P src/tsconfig.app.json src/index.ts'",
        "test:clear": "jest --clearCache",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "test:watch": "jest --coverage --watchAll",
        "test": "jest --coverage",
        "tsc": "tsc -p src/tsconfig.app.json",
        "ts-node": "ts-node -r tsconfig-paths/register -r dotenv/config",
        "webpack:run": "node -r dotenv/config build/sb-clean-blog-node.js",
        "webpack": "webpack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StartBootstrap/sb-clean-blog-node.git"
    },
    "bugs": {
        "url": "https://github.com/StartBootstrap/sb-clean-blog-node/issues"
    },
    "homepage": "https://github.com/StartBootstrap/sb-clean-blog-node#readme",
    "devDependencies": {
        "@inip/static-server": "1.0.1",
        "@start-bootstrap/sb-clean-blog-shared-types": "1.0.1",
        "@types/bcrypt": "3.0.0",
        "@types/concurrently": "5.1.0",
        "@types/jest": "25.1.2",
        "@types/jsonwebtoken": "8.3.7",
        "@types/module-alias": "2.0.0",
        "@types/node": "13.7.2",
        "@types/pino": "5.15.5",
        "@types/shelljs": "0.8.6",
        "chalk": "3.0.0",
        "concurrently": "5.1.0",
        "dotenv": "8.2.0",
        "jest": "25.1.0",
        "nodemon": "2.0.2",
        "prettier": "1.19.1",
        "shelljs": "0.8.3",
        "ts-jest": "25.2.0",
        "ts-loader": "6.2.1",
        "ts-node": "8.6.2",
        "tsconfig-paths": "3.9.0",
        "tslint": "6.0.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-plugin-prettier": "2.1.0",
        "typescript": "3.7.5",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "@inip/http-codes": "1.1.0",
        "bcrypt": "3.0.8",
        "change-case": "4.1.1",
        "crypto-random-string": "3.1.0",
        "fastify": "2.15.1",
        "fastify-cors": "3.0.2",
        "fastify-helmet": "3.0.2",
        "got": "10.5.7",
        "jsonwebtoken": "8.5.1",
        "lorem-ipsum": "2.0.3",
        "moment": "2.24.0",
        "mysql": "2.18.1",
        "p-limit": "2.2.2",
        "pg": "7.18.1",
        "pino-pretty": "3.6.0",
        "reflect-metadata": "0.1.13",
        "typeorm": "0.2.22"
    }
}
